<?xml version="1.0"?>
<launch>

  	<param name="robot_description" command="$(find xacro)/xacro '$(find five_axis_robot)/urdf/five_axis_kinect.urdf.xacro'" />

	<node name="kinect2_bridge" pkg="kinect2_bridge" type="kinect2_bridge"/>

	<node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui"/>

  	<!-- Combine joint values -->
  	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>

	<!-- Show in Rviz   -->
	<node name="rviz" pkg="rviz" type="rviz" args="-d '$(find five_axis_robot)/data/five_axis.rviz'"/>

</launch>